<web-app version="2.5"
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>APIServlet</servlet-name>
    <servlet-class>com.apigee.apimodel.servlet.APIServlet</servlet-class>
    <init-param>
      <param-name>APIModel</param-name>
      <param-value>/apitest.md</param-value>
    </init-param>
    <init-param>
      <param-name>APIMapping</param-name>
      <param-value>/apimap.properties</param-value>
    </init-param>
    <init-param>
      <param-name>VerboseErrors</param-name>
      <param-value>true</param-value>
    </init-param>
    <init-param>
      <param-name>SupportCORS</param-name>
      <param-value>true</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>APIServlet</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
